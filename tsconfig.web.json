{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/**/*.ts",
    "src/preload/*.d.ts",
    "src/shared/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "skipLibCheck": true,
    "paths": {
      "renderer/*": ["src/renderer/*"],
      "shared/*": ["src/shared/*"]
    }
  }
}
